@{
}

<h2>Courses List</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Credit</th>
            <th>Semester</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Credit</td>
                <td>@item.Semester</td>
            </tr>
        }
    </tbody>
</table>

